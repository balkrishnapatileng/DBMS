+++++++++++++++++++++++++++ Some Basic "Select Commands for practice" +++++++++++++++++++++++++++++++++++++++

Select all columns from the students table:
sql
SELECT * FROM students;

Select the first_name and last_name of all students:
sql
SELECT first_name, last_name FROM students;

Select the age of all students:
sql
SELECT age FROM students;

Select the email of all students:
sql
SELECT email FROM students;

Select the roll_number and course of all students:
sql
SELECT roll_number, course FROM students;

Select the address of all students:
sql
SELECT address FROM students;

Select the phone_number of all students:
sql
SELECT phone_number FROM students;

Select the first_name, last_name, and email of all students:
sql
SELECT first_name, last_name, email FROM students;

Select the gender of all students:
sql
SELECT gender FROM students;

Select the enrollment_date of all students:
sql
SELECT enrollment_date FROM students;

++++++++++++++++++++++++++++++ Some Advance "Select Commands for practice"  +++++++++++++++++++++++++++++++++

Select all columns for students enrolled in 'Computer Science':
sql
SELECT * FROM students WHERE course = 'Computer Science';

Select first_name and last_name for students older than 21:
sql
SELECT first_name, last_name FROM students WHERE age > 21;

Select the roll_number and email of 'Male' students:
sql
SELECT roll_number, email FROM students WHERE gender = 'Male';

Select students who enrolled on '2023-08-05':
sql
SELECT * FROM students WHERE enrollment_date = '2023-08-05';

Select first_name, last_name, and course for 'Female' students enrolled in 'Mechanical Engineering':
sql
SELECT first_name, last_name, course FROM students WHERE gender = 'Female' AND course = 'Mechanical Engineering';

Select address for students whose last_name is 'Clark':
sql
SELECT address FROM students WHERE last_name = 'Clark';

Select roll_number, first_name, and last_name for students who enrolled after '2023-09-01':
sql
SELECT roll_number, first_name, last_name FROM students WHERE enrollment_date > '2023-09-01';

Select students with a phone_number ending in '4567':
sql
SELECT * FROM students WHERE phone_number LIKE '%4567';

Select students whose first_name starts with 'J':
sql
SELECT * FROM students WHERE first_name LIKE 'J%';

Select the age and course of students enrolled on '2023-08-12':
sql
SELECT age, course FROM students WHERE enrollment_date = '2023-08-12';

Select students with first_name containing exactly 5 characters:
sql
SELECT * FROM students WHERE LENGTH(first_name) = 5;

Select students enrolled in 'Civil Engineering' and ordered by enrollment_date:
sql
SELECT * FROM students WHERE course = 'Civil Engineering' ORDER BY enrollment_date;

Select first_name, last_name, and email for students with an age of 20:
sql
SELECT first_name, last_name, email FROM students WHERE age = 20;

Select students whose address contains 'Maple':
sql
SELECT * FROM students WHERE address LIKE '%Maple%';

Select first_name, last_name, and phone_number of students who enrolled between '2023-09-01' and '2023-09-10':
sql
SELECT first_name, last_name, phone_number FROM students WHERE enrollment_date BETWEEN '2023-09-01' AND '2023-09-10';

Select students who have the same first_name and last_name:
sql
SELECT * FROM students WHERE first_name = last_name;

Select students who are either in 'Electrical Engineering' or 'Civil Engineering':
sql
SELECT * FROM students WHERE course IN ('Electrical Engineering', 'Civil Engineering');

Select the first_name, last_name, and course of 'Female' students aged 23:
sql
SELECT first_name, last_name, course FROM students WHERE gender = 'Female' AND age = 23;

Select students whose email contains 'example.com':
sql
SELECT * FROM students WHERE email LIKE '%example.com%';

Select students whose roll_number is less than 10 and age greater than 20:
sql
SELECT * FROM students WHERE roll_number < 10 AND age > 20;

Select first_name and last_name for students whose address ends with 'Ave':
sql
SELECT first_name, last_name FROM students WHERE address LIKE '%Ave';

Select the first_name and course of students who enrolled in September 2023:
sql
SELECT first_name, course FROM students WHERE enrollment_date BETWEEN '2023-09-01' AND '2023-09-30';

Select students whose phone_number starts with '0123':
sql
SELECT * FROM students WHERE phone_number LIKE '0123%';

Select students who are enrolled in 'Computer Science' and ordered by first_name:
sql
SELECT * FROM students WHERE course = 'Computer Science' ORDER BY first_name;

Select roll_number, first_name, and last_name for students whose last_name starts with 'A' or 'B':
sql
SELECT roll_number, first_name, last_name FROM students WHERE last_name LIKE 'A%' OR last_name LIKE 'B%';

Select students who enrolled on the earliest enrollment_date:
sql
SELECT * FROM students WHERE enrollment_date = (SELECT MIN(enrollment_date) FROM students);

Select students whose age is an odd number:
sql
SELECT * FROM students WHERE age % 2 <> 0;

Select students with the longest first_name:
sql
SELECT * FROM students ORDER BY LENGTH(first_name) DESC LIMIT 1;

Select first_name, last_name, and course of 'Male' students enrolled on or after '2023-08-20':
sql
SELECT first_name, last_name, course FROM students WHERE gender = 'Male' AND enrollment_date >= '2023-08-20';

Select students whose roll_number is a multiple of 3:
sql
SELECT * FROM students WHERE roll_number % 3 = 0;

Select first_name, last_name, and phone_number of students whose email ends with '1@example.com':
sql
SELECT first_name, last_name, phone_number FROM students WHERE email LIKE '%1@example.com';

Select the total number of students in the 'Mechanical Engineering' course:
sql
SELECT COUNT(*) AS total_students FROM students WHERE course = 'Mechanical Engineering';

Select students whose first_name is 'Nina' or last_name is 'Young':
sql
SELECT * FROM students WHERE first_name = 'Nina' OR last_name = 'Young';

Select roll_number, first_name, and enrollment_date for students enrolled after '2023-08-15' ordered by enrollment_date:
sql
SELECT roll_number, first_name, enrollment_date FROM students WHERE enrollment_date > '2023-08-15' ORDER BY enrollment_date;

Select students who have not enrolled in 'Civil Engineering':
sql
 SELECT * FROM students WHERE course <> 'Civil Engineering';

Select first_name and course of students who are younger than 22:
sql
SELECT first_name, course FROM students WHERE age < 22;

Select the top 5 students with the oldest enrollment_date:
sql
SELECT * FROM students ORDER BY enrollment_date LIMIT 5;

Select students with first_name containing the letter 'a':
sql
SELECT * FROM students WHERE first_name LIKE '%a%';

Select students who enrolled in 'Computer Science' and are older than 21:
sql
SELECT * FROM students WHERE course = 'Computer Science' AND age > 21;

Select students whose last_name is 'Scott' and gender is 'Female':
sql
SELECT * FROM students WHERE last_name = 'Scott' AND gender = 'Female';

Select the course and total number of students in each course:
sql
SELECT course, COUNT(*) AS student_count FROM students GROUP BY course;

Select the roll_number and first_name of students who enrolled in the first 10 days of August 2023:
sql
SELECT roll_number, first_name FROM students WHERE enrollment_date BETWEEN '2023-08-01' AND '2023-08-10';

Select students whose age is greater than the average age of all students:
sql
SELECT * FROM students WHERE age > (SELECT AVG(age) FROM students);

Select students who are enrolled in the 'Electrical Engineering' course and ordered by last_name:
sql
SELECT * FROM students WHERE course = 'Electrical Engineering' ORDER BY last_name;

Select the unique address values from the students table:
sql
SELECT DISTINCT address FROM students;

Select the first_name, last_name, and gender of students who are not in 'Mechanical Engineering':
sql
SELECT first_name, last_name, gender FROM students WHERE course <> 'Mechanical Engineering';

Select students whose phone_number contains '8901':
sql
SELECT * FROM students WHERE phone_number LIKE '%8901%';

Select students with the shortest last_name:
sql
SELECT * FROM students ORDER BY LENGTH(last_name) ASC LIMIT 1;

Select students who enrolled after '2023-09-01' and whose address contains 'Maple':
sql
SELECT * FROM students WHERE enrollment_date > '2023-09-01' AND address LIKE '%Maple%';

Select the total number of 'Female' students in the 'Civil Engineering' course:
sql
SELECT COUNT(*) AS total_female_students FROM students WHERE gender = 'Female' AND cour
